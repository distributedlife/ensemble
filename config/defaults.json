{
  "client": {
    "aspectRatio": 2.6,
    "widescreenMinimumMargin": 32,
    "element": "canvas",
    "inputElement": "input"
  },
  "server": {
    "physicsUpdateLoop": 15,
    "pushUpdateFrequencyMs": 45
  },
  "logging": {
    "logLevel": "warn",
    "silencedPlugins": [
      "CurrentState",
      "GamesList",
      "LowestInputProcessed",
      "NewState",
      "OnInput",
      "OnPhysicsFrame",
      "RawStateAccess",
      "StateAccess",
      "StateMutator",
      "Time",
      "Config"
    ],
    "expressBunyanLogger": {
      "excludes": [
        "req",
        "res",
        "res-headers",
        "response-hrtime",
        "short-body",
        "req-headers",
        "incoming",
        "req_id",
        "body",
        "user-agent",
        "os"
      ]
    }
  },
  "debug": {
    "inputOverlay": false,
    "network": false,
    "time": false
  },
  "measure": {
    "timers": []
  }
}