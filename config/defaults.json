{
  "server": {
    "physicsUpdateLoop": 15,
    "pushUpdateFrequencyMs": 45
  },
  "logging": {
    "logLevel": "warn",
    "silencedPlugins": [
      "GamesList",
      "ServerSideUpdate",
      "StateAccess",
      "RawStateAccess",
      "StateMutator",
      "NewState",
      "CurrentState",
      "OnInput"
    ],
    "expressBunyanLogger": {
      "excludes": [
        "req",
        "res",
        "res-headers",
        "response-hrtime",
        "short-body",
        "req-headers",
        "incoming",
        "req_id",
        "body",
        "user-agent",
        "os"
      ]
    }
  },
  "debug": {
    "inputOverlay": false
  }
}