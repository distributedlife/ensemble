{
  "game": {
    "id": "ensemble+notset",
    "name": "Not Set"
  },
  "client": {
    "aspectRatio": 2.6,
    "widescreenMinimumMargin": 32,
    "element": "canvas",
    "inputElement": "input",
    "physicsUpdateLoop": 15,
    "clientSidePrediction": true,
    "input": {
      "gamepad": {
        "deadzoneAlgorithm": "radial",
        "deadzoneMapper": "normalised"
      }
    }
  },
  "server": {
    "physicsUpdateLoop": 15,
    "pushUpdateFrequency": 45
  },
  "ensemble": {
    "minPlayers": 1,
    "maxPlayers": 1,
    "dashboard": false,
    "maxSavesPerPlayer": 1,
    "autoSaveBehaviour": "persistent",
    "locales": ["en"]
  },
  "mongo": {
    "endpoint": "mongodb://localhost:27017/ensemble"
  },
  "logging": {
    "logLevel": "warn",
    "silencedPlugins": [
      "$",
      "AfterPhysicsFrame",
      "BeforePhysicsFrame",
      "CollisionDetectionSystem",
      "Config",
      "CurrentState",
      "SavesList",
      "LowestInputProcessed",
      "On",
      "OnIncomingClientInputPacket",
      "OnInput",
      "OnOutgoingServerPacket",
      "OnPhysicsFrame",
      "PhysicsSystem",
      "RawStateAccess",
      "StateAccess",
      "StateMutator",
      "Time"
    ],
    "expressBunyanLogger": {
      "excludes": [
        "req",
        "res",
        "res-headers",
        "response-hrtime",
        "short-body",
        "req-headers",
        "incoming",
        "req_id",
        "body",
        "user-agent",
        "os"
      ]
    }
  },
  "debug": {
    "develop": false,
    "enabled": false,
    "fps": false,
    "input": false,
    "multiplayer": false,
    "network": false,
    "startOpen": false,
    "time": false,
    "gamepad": false,
    "virtualGamepad": true
  },
  "measure": {
    "endpoint": "http://localhost:4000",
    "frequency": 1,
    "timers": [
      "ensemblejs.StateMutator.*",
      "ensemblejs.socket.*"
    ],
    "timersIgnored": [
    ]
  }
}