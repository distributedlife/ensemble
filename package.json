{
  "name": "ensemblejs",
  "version": "6.2.4",
  "description": "The ensemble game framework",
  "repository": {
    "type": "git",
    "url": "https://github.com/ensemblejs/ensemblejs.git"
  },
  "keywords": [
    "cross-browser",
    "ensemble",
    "framework",
    "game",
    "gamedev",
    "multi-modal",
    "multi-device",
    "device",
    "gamepad",
    "controller",
    "multiplayer",
    "singleplayer",
    "online",
    "games"
  ],
  "scripts": {
    "start": "NODE_ENV=test gulp",
    "build": "gulp framework:build",
    "unit": "NODE_ENV=test gulp framework:test:unit",
    "test": "NODE_ENV=test gulp framework:test",
    "perf": "NODE_ENV=test gulp framework:test:perf",
    "perf-mon": "NODE_ENV=test nodejs-dashboard gulp framework:test:perf-mon",
    "karma-perf": "karma start perf.conf.js",
    "int": "NODE_ENV=test gulp framework:test:integration",
    "convert": "babel src --out-dir lib --sourceMaps inline --comments --copy-files --ignore spec.js",
    "prepublish": "npm run build && npm run convert",
    "coverage": "codecov"
  },
  "author": "Ryan Boucher <ryan.boucher@distributedlife.com> (http://distributedlife.com)",
  "contributors": [
    "Andy Shaw <shawry@shawry.com>",
    "Pam Rucinque <pamela.rucinque@gmail.com>"
  ],
  "bugs": {
    "url": "https://github.com/ensemblejs/ensemble/issues"
  },
  "homepage": "http://ensemblejs.com",
  "engines": {
    "node": "^4.x",
    "npm": "^3.x"
  },
  "files": [
    "client.js",
    "server.js",
    "config/",
    "locales/",
    "public/",
    "lib/",
    "tasks/"
  ],
  "browserify": {
    "transform": [
      "envify",
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ],
          "plugins": [
            "transform-object-rest-spread"
          ]
        }
      ],
      "require-globify",
      [
        "pugify",
        {
          "compileDebug": false,
          "pretty": false
        }
      ],
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "window": "global:window"
  },
