{
  "name": "ensemblejs",
  "version": "6.0.0-develop-35",
  "description": "The ensemble game framework",
  "repository": {
    "type": "git",
    "url": "https://github.com/ensemblejs/ensemblejs.git"
  },
  "keywords": [
    "cross-browser",
    "ensemble",
    "framework",
    "game",
    "gamedev",
    "multi-modal"
  ],
  "scripts": {
    "start": "NODE_ENV=test gulp",
    "unit": "NODE_ENV=test gulp framework:test:unit",
    "test": "NODE_ENV=test gulp framework:test",
    "int": "NODE_ENV=test gulp framework:test:integration",
    "prepublish": "babel src --out-dir lib --sourceMaps inline --comments --copy-files",
    "coverage": "codecov"
