  "homepage": "http://ensemblejs.com",
  "engines": {
    "node": "^4.x",
    "npm": "^3.x"
  },
  "files": [
    "client.js",
    "server.js",
    "config/",
    "locales/",
    "public/",
    "lib/",
    "tasks/"
  ],
  "browserify": {
    "transform": [
      "envify",
      [
        "babelify",
        {
          "presets": [
            "es2015"
